<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h4 class="page-title">My Profile</h4>
    </div>
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">
            <li><a href="~/Home/index">Laundry</a></li>
            <li class="active">My Profile</li>
        </ol>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="row">
    <div class="col-sm-12">
        <div class="white-box">

            @*<%-- <h3 class="box-title">Basic Information</h3>--%>*@
            <div class="form-material form-horizontal" style="width: 100%">

                <div class="form-group ">
                    <label class="col-md-2">Old Password</label>
                    <div class="col-md-4">
                        <input type="text" id="example-text1" name="example-text" class="form-control" placeholder="enter your old password" />
                    </div>
                </div>

                <div class="form-group ">
                    <label class="col-md-2">New Password</label>
                    <div class="col-md-4">
                        <input type="text" id="example-text2" name="example-text" class="form-control" placeholder="enter your new password" />
                    </div>
                </div>
                <div class="form-group ">
                    <label class="col-md-2">Confirm Password</label>
                    <div class="col-md-4">
                        <input type="text" id="example-text3" name="example-text" class="form-control" placeholder="enter your confirm password" />
                    </div>
                </div>


                <div class="form-group col-md-4">
                    <label></label>
                    <div class="m-l-10 m-t-5">
                        <i class="btn btn-info waves-effect waves-light  waves-input-wrapper" style="">
                            <button type="button" id="btnChangePassword" class="waves-button-input">Change Now</button>
                        </i>
                    </div>
                </div>

                <div class="form-group  col-md-12">
                    <label class="col-md-2"> Last Time Password Reset</label>
                    <label class="col-md-4">
                        23-10-2015 16:34:45
                    </label>
                </div>
            </div>

        </div>

    </div>

</div>

